{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "A TypeScript template",
  "keywords": [
    "typescript"
  ],
  "homepage": "https://github.com/p-chan/ts-template",
  "bugs": {
    "url": "https://github.com/p-chan/ts-template/issues"
  },
  "license": "MIT",
  "author": "P-Chan",
  "main": "./dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/p-chan/ts-template.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "dev": "nodemon --watch \"./src/**/*\" --exec \"ts-node\" ./src/index.ts",
    "format": "prettier --write .",
    "start": "node ."
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@p-chan/prettier-config": "^2.0.0",
    "@p-chan/tsconfig": "^1.1.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.0",
    "typescript": "^4.0.3"
  },
  "private": true
}
